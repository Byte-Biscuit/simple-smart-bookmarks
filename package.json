{
    "name": "simple-smart-bookmarks",
    "displayName": "Simple Smart Bookmarks",
    "version": "1.2.1",
    "description": "Simplify and be more efficient",
    "author": "Biscuit Zhou",
    "scripts": {
        "dev": "plasmo dev",
        "build": "plasmo build",
        "intl:extract": "react-intl-universal-extract --cmd extract --source-path ./src --output-path ./src/locales/en-us.json --verbose"
    },
    "dependencies": {
        "@ant-design/cssinjs": "1.10.1",
        "@ant-design/icons": "^5.1.4",
        "@plasmohq/storage": "^1.10.0",
        "@types/lodash": "^4.14.195",
        "antd": "5.6.0",
        "cheerio": "1.0.0-rc.12",
        "classnames": "^2.3.2",
        "dayjs": "^1.11.8",
        "lodash": "^4.17.21",
        "plasmo": "0.89.2",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-intl-universal": "^2.6.18"
    },
    "devDependencies": {
        "@plasmohq/prettier-plugin-sort-imports": "4.0.1",
        "@types/chrome": "0.0.237",
        "@types/node": "20.2.5",
        "@types/react": "18.2.8",
        "@types/react-dom": "18.2.4",
        "autoprefixer": "^10.4.14",
        "postcss": "^8.4.31",
        "react-intl-universal-extract": "^1.0.6",
        "tailwindcss": "^3.3.2",
        "typescript": "5.1.3"
    },
    "manifest": {
        "permissions": [
            "bookmarks",
            "activeTab"
        ],
        "host_permissions": [
            "*://*/*"
        ]
    }
}
